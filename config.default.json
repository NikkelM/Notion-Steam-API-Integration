{
	"$schema": "config.schema.json",
	"notionIntegrationKey": "<secretKey>",
	"notionDatabaseId": "<databaseId>",
	"updateInterval": 60000,
	"steamAppIdProperty": "Steam App ID",
	"forceReset": false,
	"gameProperties": {
		"gameName": {
			"enabled": true,
			"notionProperty": "Name",
			"isPageTitle": true
		},
		"coverImage": true,
		"gameIcon": true,
		"releaseDate": {
			"enabled": true,
			"notionProperty": "Release Date",
			"format": "date"
		},
		"reviewScore": {
			"enabled": true,
			"notionProperty": "Review Score"
		},
		"tags": {
			"enabled": true,
			"notionProperty": "Tags",
			"tagLanguage": "english"
		},
		"gameDescription": {
			"enabled": true,
			"notionProperty": "Description"
		},
		"storePage": {
			"enabled": true,
			"notionProperty": "Store Page"
		},
		"gamePrice": {
			"enabled": true,
			"notionProperty": "Price"
		},
		"steamDeckCompatibility": {
			"enabled": true,
			"notionProperty": "Steam Deck Compatibility"
		}
	}
}